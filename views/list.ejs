<%- include("header") -%>


      <div class="box" id="heading">
        <h1> <%= listtitle %></h1>
      </div>
        
      <div class="box">
          <%  for(let i=0; i<NewListItems.length; i++){  %>
            <div class="item">
              <input type="checkbox" >
              <p> <%= NewListItems[i] %></p>
            </div>
            
         <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="input" placeholder="Enter your todo" autocomplete="off">
            <button type="submit" name="list" value= <%= listtitle %> > + </button>
        </form>       


      </div>
    
      <%- include("footer") -%>